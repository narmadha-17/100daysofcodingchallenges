// divisibility

#include <stdio.h>
int main(){
    int n;
    int a;
    scanf("%d",&a);
    for(int i=0;i<a;i++){
        scanf("%d",&n);
    }
    if(n%10==0){
        printf("Yes");
    }
    else{
        printf("No");
    }
    return 0;
}


//squarts
#include <stdio.h>
int main() {
    int t,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&x);
        printf("%d\n",x*15);
    }
	return 0;
}
