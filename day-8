//life the universe and everything

#include<stdio.h>
int main(){
   int a;
  
   while(1){
       scanf("%d",&a);
       if(a==42)
           break;
           printf("%d\n",a);
      


   }
   return 0;
}


//Toggle string

#include<string.h>
void main(){
     char s[100],c;
     int i,x;
     scanf("%s",s);
     for(i=0;i<=strlen(s)-1;i++){
        x=s[i];
        if(x<=90)
          x=x+32;
        else
          x=x-32;
        c=x;
        s[i]=c;
     }
     printf("%s",s);
   }
