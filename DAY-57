//MISSING NUMBER
//LEETCODE
//JAVA

class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=i){
                return i;
            }
            if(i==nums.length-1){
                return i+1;
            }
        }
        return 0;
    }
}
