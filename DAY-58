//AVERAGE SALARY EXCLUDING MINIMUM AND MAXIMUM SALARY
//LEETCODE
//JAVA
class Solution {
    public double average(int[] salary) {
        Arrays.sort(salary);
        int i;
        double sum=0;
        for( i=1;i<salary.length-1;i++){
            sum=sum+salary[i];
        }
        return sum/(salary.length-2);        
    }
}
